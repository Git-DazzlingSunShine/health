<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Sql">
    <sql id="checkItem">
        `id`,
        `code`,
        `name`,
        `sex`,
        `age`,
        `price`,
        `type`,
        `attention`,
        `remark`
    </sql>

    <sql id="checkGroup">
        `id`,
        `code`,
        `name`,
        `sex`,
        `helpCode`,
        `remark`,
        `attention`
    </sql>

    <sql id="setmeal">
        `id`,
        `code`,
        `name`,
        `sex`,
        `helpCode`,
        `age`,
        `price`,
        `remark`,
        `attention`,
        `img`
    </sql>
</mapper>